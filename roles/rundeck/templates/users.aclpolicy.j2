description: user, use jobs
context:
  project: 'mypaas'
for:
  resource:
    - allow: '*' # allow read/create all kinds
  adhoc:
    - allow: '*' # allow read/running/killing adhoc jobs
  job:
    - allow: '*' # allow read/write/delete/run/kill of all jobs
  node:
    - allow: '*' # allow read/run for all nodes
by:
  group: developpers

---

description: Admin, all access.
context:
  application: 'rundeck'
for:
  project:
    - match:
        name: 'mypaas'
      allow: [read]
  system:
    - match:
        name: '.*'
      allow: [read]
by:
  group: developpers

